
@if(Context.User.Identity.IsAuthenticated == false)
{
    <li class="nav-item">
        <a asp-controller="Empleados" asp-action="PerfilEmpleado">Log In</a>
    </li>
}
else
{
    <li class="nav-item">
        <a asp-controller="Manager" asp-action="Logout">Log Out <span style="color:darkorchid">@Context.User.Identity.Name</span></a>
    </li>
}

